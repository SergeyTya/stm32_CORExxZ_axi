PREAMBLE=if [ -f gen_source.pl ]; then perl gen_source.pl; fi
POSTAMBLE=if [ -f Makefile.lmi ]; then make -f Makefile.lmi; fi
LIBRARY=yes
TARGET=IQmathLib
GUI_TARGET=IQmathLib
USE_TARGET_NAMES=1
DESC=IQmath library
ROOT=..
COPYRIGHT=2010
VPATH=gnu
SRC=./atoIQN.c
SRC+= ./IQNtoa.c
