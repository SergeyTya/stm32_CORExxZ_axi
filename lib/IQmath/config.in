SRC+= ./ti/IQNdiv.S
SRC+= ./ti/IQNexp.S
SRC+= ./ti/IQNexp2.S
SRC+= ./ti/IQNfrac.S
SRC+= ./ti/IQNint.S
SRC+= ./ti/IQNisqrt.S
SRC+= ./ti/IQNmag.S
SRC+= ./ti/IQNmpy.S
SRC+= ./ti/IQNmpyI32frac.S
SRC+= ./ti/IQNmpyI32int.S
SRC+= ./ti/IQNrmpy.S
SRC+= ./ti/IQNrsmpy.S
SRC+= ./ti/IQNsin.S
SRC+= ./ti/IQNsinPU.S
SRC+= ./ti/IQNsqrt.S
SRC+= ./ti/IQNtoD.S
SRC+= ./ti/IQNtoF.S
SRC+= ./ti/IQtables.S
SRC+= ./ti/IQxmpy.S
CR_EXCLUDE={(iar|keil|ti)}
CR_EXCLUDE_INT={(templates|IQtest)}
CS_EXCLUDE=*.pl|*.in|iar|IQtest|keil|templates|ti
